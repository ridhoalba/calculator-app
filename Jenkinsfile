pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo'Sedang build'
            }
        }
        stage('Tests') {
            steps {
                echo'Testing'
            }
        }
        stage('Release') {
            steps {
                echo'Release'
            }
        }
        stage('Deploy') {
            steps {
                echo'Deploy to Production'
            }
        }
        stage('Monitor') {
            steps {
                echo'Monitoring'
            }
        }
    }
}